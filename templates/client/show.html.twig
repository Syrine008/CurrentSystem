{% extends 'base.html.twig' %}
{% block title %}Afficher Client{% endblock %}
{% block body %}
    <h1>{{ client.nom }}</h1>
    <h1>{{ client.prenom }}</h1>
    <p>{{ client.sexe }}</p>
    <p>{{ client.IdType }}</p>
    <p>{{ client.IdFile }}</p>
    <p>{{ client.DateDelivre|date('Y-m-d') }}</p>
    <p>{{ client.NumPass }}</p>
    <p>{{ client.DateNaissance|date('Y-m-d') }}</p>
    <p>{{ client.nationalite }}</p>
    <p>{{ client.adresse }}</p>
    <p>{{ client.telephone }}</p>
    <hr>
    <a href="/client">Retour</a>
{% endblock %}
